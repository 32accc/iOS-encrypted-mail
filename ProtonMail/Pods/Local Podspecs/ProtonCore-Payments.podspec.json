    "ProtonCore-Authentication": [
      "12.0.0"
    ],
    "ProtonCore-Networking": [
      "12.0.0"
    ],
    "ProtonCore-Services": [
      "12.0.0"
    ],
    "ProtonCore-Subscriptions": [
      "12.0.0"
    ]
