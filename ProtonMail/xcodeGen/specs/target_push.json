{
   "targets":{
      "Push":{
         "type":"app-extension",
         "platform":"iOS",
         "sources":[
            "Push/NotificationViewController.swift"
         ],
         "dependencies":[
            {
               "sdk":"UserNotifications.framework"
            },
            {
               "sdk":"UserNotificationsUI.framework"
            }
         ],
         "configFiles":{
            "Production Debug":"xcodeGen/xcconfig/Push-Debug.xcconfig",
            "Production Release":"xcodeGen/xcconfig/Push-Release.xcconfig",
            "Enterprise Debug":"xcodeGen/xcconfig/Push-EnterpriseDebug.xcconfig",
            "Enterprise Release":"xcodeGen/xcconfig/Push-Enterprise.xcconfig"
         },
         "settings":{
            "configs":{
               "Production Debug":{
                  "INFOPLIST_FILE":"Push/Info.plist"
               },
               "Production Release":{
                  "INFOPLIST_FILE":"Push/Info.plist"
               },
               "Enterprise Debug":{
                  "INFOPLIST_FILE":"Push/InfoDev.plist"
               },
               "Enterprise Release":{
                  "INFOPLIST_FILE":"Push/InfoDev.plist"
               }
            }
         }
      }
   }
}