{
   "name": "ProtonMail",
   "options": {
      "createIntermediateGroups": true,
      "deploymentTarget": {
         "iOS": "15.0"
      },
      "postGenCommand": "xcodeGen/post_gen.sh"
   },
   "fileGroups": [
      "ProtonMail",
      "PushService",
      "Share",
      "Siri",
      "ProtonMailTests",
      "ProtonMailUITests"
   ],
   "configs": {
      "Debug": "debug",
      "Release": "release",
      "Adhoc": "release"
   },
   "packages": {
      "Difference": {
         "url": "https://github.com/krzysztofzablocki/Difference.git",
         "from": "1.0.0"
      },
      "Factory": {
         "url": "https://github.com/hmlongco/Factory.git",
         "from": "2.0.0"
      },
      "LifetimeTracker": {
         "url": "https://github.com/krzysztofzablocki/LifetimeTracker.git",
         "from": "1.0.0"
      },
      "OHHTTPStubs": {
         "url": "https://github.com/AliSoftware/OHHTTPStubs",
         "from": "9.1.0"
      },
      "ProtonCoreEventsLoop": {
         "path": "LocalPackages/ProtonCoreEventsLoop"
      },
      "SwiftCSSParser": {
         "url": "https://github.com/stackotter/swift-css-parser.git",
         "from": "0.0.0"
      }
   },
   "configFiles": {
      "Debug": "xcodeGen/xcconfig/Project-Debug.xcconfig",
      "Release": "xcodeGen/xcconfig/Project-Release.xcconfig",
      "Adhoc": "xcodeGen/xcconfig/Project-Release.xcconfig"
   }
}
