{
   "name": "ProtonMail",
   "options": {
      "createIntermediateGroups": true,
      "deploymentTarget": {
         "iOS": "14.7"
      },
      "postGenCommand": "xcodeGen/post_gen.sh"
   },
   "fileGroups": [
      "ProtonMail",
      "PushService",
      "Share",
      "Siri",
      "ProtonMailTests",
      "ProtonMailUITests"
   ],
   "configs": {
      "Debug": "debug",
      "Release": "release",
      "Enterprise Debug": "debug",
      "Enterprise Release": "release"
   },
   "packages": {
      "Difference": {
         "url": "https://github.com/krzysztofzablocki/Difference.git",
         "version": "1.0.2"
      },
      "Factory": {
         "url": "https://github.com/hmlongco/Factory.git",
         "version": "2.2.0"
      },
      "LifetimeTracker": {
         "url": "https://github.com/krzysztofzablocki/LifetimeTracker.git",
         "version": "1.8.1"
      },
      "ProtonCoreEventsLoop": {
         "path": "ProtonCoreEventsLoop"
      },
      "SwiftCSSParser": {
         "url": "https://github.com/stackotter/swift-css-parser.git",
         "version": "0.1.2"
      }
   },
   "configFiles": {
      "Debug": "xcodeGen/xcconfig/Project-Debug.xcconfig",
      "Release": "xcodeGen/xcconfig/Project-Release.xcconfig",
      "Enterprise Debug": "xcodeGen/xcconfig/Project-EnterpriseDebug.xcconfig",
      "Enterprise Release": "xcodeGen/xcconfig/Project-Enterprise.xcconfig"
   }
}
